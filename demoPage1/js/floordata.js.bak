$('.floor-content div p').hover(function(){
    $(this).addClass("adjust-space")
})

function tresetFontSize(wordbox,minSize,maxHeight,maxSize){                 //控制字体大小变化,但div大小不变的函数
                    
    wordbox.css('font-size', minSize + "px");
    maxSize++;
    wordbox.each(function(){
                   for (var i = minSize; i < maxSize; i++){
                                               if ($(this).height() > maxHeight){
                                                        $(this).css('font-size', (i - 2) + 'px');
                                                        break;
                                               }else{
                                                       $(this).css('font-size', i + 'px');
                                               }
                   }

    })

}   

