*{
	margin: 0;
	padding: 0;
	list-style: none;
	box-sizing:border-box;
}
.displayNone{ display: none;}
a{
	text-decoration: none;
}
body{
	font-family: Helvetica Neue,Helvetica,PingFang SC,Hiragino Sans GB,Microsoft YaHei,\\5FAE\8F6F\96C5\9ED1,Arial,sans-serif;
	font-size: 14px;
}
.clearfix{
            clear: both;
            width: 0;
            height: 0;
            zoom:1;
}
.box{
	margin: 0 auto;
	max-width: 6.4rem;
	/*padding-bottom: 70px;*/
	background: #f0f0f0;
	position: relative;
	border: 0.01rem solid lightgray;	
	background-color:lightgreen;
	
}

/*---------banner-------*/
.box .banner{
	width: 100%;
	height:0.55rem;
	background-color: yellow; 
	background: url(../img/logo.png) center no-repeat;
	background-size: 100% 0.55rem;
}

/*-----------search样式---------------*/
.search{
	width: 6.4rem;
	height: 0.55rem;
	background-color: orange;
	background: url(../img/ui-search.jpg)center no-repeat;
	background-size: 6.4rem 0.55rem;
	position: relative;
	
}
.ui-search-selected{
	width: 1.34rem;
	height: 0.55rem;
	line-height: 0.55rem;	
	font-size: 0.19rem;
	color: white;	
	cursor: pointer;
	padding-left: 0.1rem;
	position: absolute;
	top: 0;
	left: 0;
	font-weight: bold;
}
.search input{
	display: block;
	width: 4.21rem;
	height: 0.50rem;
	color: rgb(216,216,216);
	position: absolute;
	left: 1.35rem;;
	top: 0.03rem;
	font-size: 0.18rem;
	border: none;
}
input[name="text"]{
       color:red
}

}
.search .ui-search-select-list{
	position: absolute;
	top: 0.55rem;
	
	
	
}
.search .ui-search-select-list a{
	display: block;
	width:1.34rem;
	height: 0.48rem;
	background-color: white;
	text-align: center;
	line-height: 0.48rem;
	font-size: 0.15rem;
	color: gray;
	border:1px solid red;
	z-index:10;
}
.search .ui-search-select-list a:hover{
	background-color: lightgray;
	color: white;
}
.ui-search-select-list{
	z-index: 9;
	display: none;
}
.ui-search-submit{
	display: block; 
	width: 0.40rem; 
	height: 0.38rem;
}
/*-----------nav样式---------------*/
.nav{
	width: 100%;
	height: 0.47rem;
	background-color: rgb(0,162,232);
	display: flex;
}
.nav .link{
	display: block;
	height: 0.47rem;
	font-size: 0.25rem;
	color: white;	
	width: 50%;
	text-align: center;
	line-height: 0.47rem;

}
.nav .link:hover{
	background-color: rgb(0,131,187);
}

/*----------Banner--Banner-slider--------*/
.banner-slider{position: relative; overflow: hidden;}
.banner-slider img{width: 6.4rem; height: 3.83rem ; display: inline-block;}
.banner-slider-wrap{
	width: 6.4rem;
	position: relative;
	height: 3.83rem;
	background-color: pink;
	top: 0;
	left: 0;
}
.banner-slider-wrap .item{
	position: absolute;
	display: block;
	top: 0;	
}
.banner-slider-wrap .item img{
	width: 6.4rem; height: 3.83rem;
}

.banner-slider-wrap .item:nth-child(1){
          left: 0
}
.banner-slider-wrap .item:nth-child(2){
          left: 6.4rem;
}
.banner-slider-wrap .item:nth-child(3){
          left: 12.8rem
}

.ui-slider-process{
          position: absolute;
          z-index: 12;
          height: 19px;
          bottom: 20px;
          margin-left: 50%;
          left: -0.51rem;
                
}
.ui-slider-process .item{
	display: inline-block;
	width: 0.19rem;
	height: 0.19rem;
	margin-right: 0.15rem;
	background-color: rgb(0,162,232);
	box-shadow: 0 0 0 0.03rem white inset;
           border-radius: 50%;
	
}
.ui-slider-process .item_focus{
	background-color: white;
	box-shadow: 0 0 0 0.04rem rgb(0,162,232) inset;
	width: 0.23rem; height: 0.23rem;
}

/*--------------------------------content样式------------------------------*/


/*------------------content-tab------------------*/
#content{
       width: 100%;            
       position: absolute;
       top: 5.4rem;

}
.content-tab{
	width: 100%;	
	background-color: yellow;	
	position: relative;
}
.content-tab .caption{
	width: 100%; 
	height: 0.55rem;
	background-color:#f0f0f0;   
	display: flex;       
}
.content-tab .caption .item{
	display: block;
            text-align: center;
            width: 50%;
            height: 0.55rem;
            line-height: 0.55rem;
            font-size: 0.25rem;
            color: rgb(0,162,232);
}
.content-tab .caption .item_focus{
	background-color: rgb(0,162,232);
	color: white;
}
#blockA{ } 
#blockB{ display: none; }
	


#blockA .block-caption{
	width: 100%;
            height: 0.45rem;
            display: flex;
            justify-content:space-around;
            
}
#blockB .block-caption{
	width: 100%;
            height: 0.45rem;
            display: flex;
            justify-content:space-around;
            
           
}
#blockA .block-caption .block-caption-item,
#blockB .block-caption .block-caption-item{
             display: block;
             padding-left: 0.06rem;
             padding-right: 0.06rem;
             font-size: 0.2rem;
             color: gray;
             float: left;
             font-weight: bold;
             line-height: 0.45rem;
             margin-top: 0.06rem;
}
#blockB .block-caption .block-caption-item_focus,
#blockA .block-caption .block-caption-item_focus{
	background-color: rgb(0,162,232);
	color: white;
}
#blockB .block-content,
#blockA .block-content{
             padding-top: 0.1rem;
             
}
#blockB .block-content .block-list-item,
#blockA .block-content .block-list-item{
	width: 100%;
	height: 1.73rem;
	margin-bottom: 0.06rem;
	padding: 0 0 0 1.9rem;
	position: relative;
	padding-top: 0.09rem;
	padding-bottom: 0.09rem;
}
#blockB .block-content .block-list-item div,
#blockA .block-content .block-list-item div{
       color: rgb(0,162,232);
       padding-left: 0.1rem;
       font-size: 0.2rem;     
       margin-bottom: 0.2rem;
}
#blockB .block-content .block-list-item img,
#blockA .block-content .block-list-item img{
	width: 1.8rem;
	height: 1.62rem;
	position: absolute;
	left: 0.05rem;
	top: 0.06rem;
}
#blockB .block-list .block-list-item .item-name span,
#blockA .block-list .block-list-item .item-name span{
      margin-left: 2.1rem;
      font-size: 0.24rem;     
}
#blockA .block-list .block-list-item .item-phone,
#blockA .block-list .block-list-item .item-address{
           	color: gray;
}
#blockB .block-list .block-list-item .item-phone,
#blockB .block-list .block-list-item .item-address{
           	color: gray;
}
.block-list-item:hover{
	background-color: rgb(232,248,255);
}

#sub-content{
width: 100%;
position:  relative;
font-size: 0.18rem;
height: 7rem;
display: none;
}
#sub-content .detail{ width: 6rem; position: absolute; top: 4.6rem; left: 0.3rem}
#sub-content .detail p{ margin-bottom: 0.10rem}
#sub-content .detail .location{
           background:url( ../img/icon-web.png) no-repeat;
           background-position: 1px -17px;
           padding-left: 0.4rem;

}
#sub-content .detail .address a{
           background:url( ../img/icon-web.png) no-repeat;
	background-position: 1px 1.4px;
	padding-left: 0.4rem;

}
#sub-content .map{ width: 6rem; height: 4rem; position: absolute;  left: 0.2rem; border: 0.01rem solid red; text-align: center; }
#sub-content .map iframe{ margin: 0 auto; }
#sub-content .sub-content-pic{ position: absolute; top: 0; left: 1.05rem; margin-top: 0.15rem; }
#sub-content .sub-content-pic img{ width: 4.3rem; height: 4.3rem;  }

/*-----------------以上content-tab------------------*/

/*-----------------------------------banner-search----------------------------------------*/

