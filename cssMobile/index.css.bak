 }

/*-------------content-news-------------*/
.content-news{
	position: absolute;
	top:18.78rem;
	width: 100%;
	height: 2.65rem;
	border:0.02rem solid rgb(234,234,234);
}
.content-close{
	position: absolute;
	top: 21.49rem;
	width: 100%;
	height: 3.25rem;
	border:0.02rem solid rgb(234,234,234);
}
.content-close .caption,
.content-news .caption{ 
	width: 100%; 
	background-color: rgb(211,211,211);
	height: 0.53rem;
	color: rgb(0,162,232);
	font-size: 0.35rem;
	line-height: 0.53rem;
	padding-left: 0.3rem;
 }
 .content-close .caption{ color: orange}
 .content-close .caption a,
 .content-news .caption a{ font-size: 0.2rem; color: gray;margin-left: 2.2rem}
 .content-close .list,
 .content-news .list{ position: absolute; top: 0.13rem;  margin-top: 0.5rem;  width: 90%; left: 4%;}
 .content-close .list a,
 .content-news .list a{ display: block; margin-bottom: 0.1rem; font-size: 0.18rem; color: gray;}

@media screen and ( max-width: 600px){
		 .content-close .list a,
		 .content-news .list a{
			  white-space:nowrap;
			  -o-text-overflow:ellipsis; 
			   -moz-binding:url('ellipsis.xml#ellipsis'); 
			   text-overflow:ellipsis; 
			   font-size: 0.12rem; 


		 }

		 .content-close{
		 	top: 23rem;
		 	height: 4.6rem;
		 	width: 100%;
		 	border: 1px solid lightgray;
		 	overflow: hidden;

		 	
		 }
		 .content-news{
		 	height: 4rem;
		 	width: 100%;
		 	border: 1px solid lightgray;
		 	overflow: hidden;
		 }

		  .content-close .list,
                            .content-news .list{ position: absolute; top: 0.13rem;  margin-top: 0.5rem;  width: 90%; left: 1%;}

                           
}


/*-----------------footer------------------*/
footer{  
	margin-top: 0.1rem;
	width: 100%;
	height: 0.6rem;
	background-color:gray;	
	font-weight: bold;
	position: absolute;
	top:  2.65rem;
}
@media screen and ( max-width: 600px){
	footer{
		 top: 4rem;
	}
            footer .wrap div{
            	             margin-top: 0.04rem;
            }
            footer .wrap div{

             }
}
footer .wrap{
	display: flex;
	justify-content :space-around;
	align-items:center;

}
footer .wrap div{
           height: 0.3rem;
           margin-top: 0.17rem;
           color: white;
           font-weight: bold;
           text-align: center;
           line-height: 0.3rem;
          
}
footer .wrap div a{
	font-size: 0.17rem;
	margin-right: 0.2rem;
	font-weight: bold;
	color: white;
}
footer .wrap div a:after{
	content:'|';
	font-size: 0.15rem;
	margin-right: 0.08rem;
	margin-left: 0.08rem;
	font-weight: none;
}

@media screen and ( max-width: 375px){
             footer .wrap div a{
             	font-size: 0.13rem;
             	margin-right: 0.2rem;
             }
             footer .wrap div a:after{
             	content: none;
             }
}

footer .wrap div a:nth-child(4):after{
	content:none;
}
footer .wrap div a:hover{
	color: lightgray;
}